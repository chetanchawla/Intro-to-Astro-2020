   "source": [
    "fig = plt.figure()\n",
    "\n",
    "x = 5*np.random.random(10) # 10 randomly spaced points between 0 and 5 (as np.random.random(n) returns \n",
    "# n random values between 0 and 1 \n",
    "y = 2.5*x + 0.8  # A simple linear equation\n",
    "\n",
    "# Real data tends to be \"noisy\" - let's simulate that here\n",
    "\n",
    "err = 1.5 # Size of errorbars - play around with this parameter!\n",
    "noise = np.random.normal(scale=err, size=len(x)) # Gaussian/normal \"noise\"\n",
    "y_data = y + noise # Adding the noise to the y-values to simulate noisy data\n",
    "\n",
    "plt.plot(x, y, color='g')    # Makes a line, color='k' is black, g is green\n",
    "plt.errorbar(x, y_data, yerr=err, fmt='o', \n",
    "             capsize=5, color='b') # Blue colored points\n",
    "\n",
    "plt.xlabel('x [arbitrary units]')\n",
    "plt.ylabel('y [arbitrary units]')\n",
    "\n",
    "# We can also set some other formatting options, like axis limits\n",
    "plt.xlim(0-0.1, max(x)+0.1)  # x-axis range from 0 (lower limit) to 5 (upper limit)\n",
    "plt.ylim(min(y_data)-err-0.1, max(y_data)+err+0.1) # Same for y-axis\n",
    "\n",
    "#plt.show()"
   ]
